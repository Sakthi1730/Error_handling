package Exception_and_Error_Handling;

import java.util.Map;
import java.util.TreeMap;
import java.util.List;
import java.util.ArrayList;
import java.util.Collections;

public class EmployeeTreeMap {
    public static void main(String[] args) {
        TreeMap<Integer, String> employees = new TreeMap<>();
        employees.put(101, "Zara");
        employees.put(102, "Alex");
        employees.put(103, "Mike");

        List<String> names = new ArrayList<>(employees.values());
        Collections.sort(names);

        System.out.println("Employees in alphabetical order:");
        for (String name : names) {
            System.out.println(name);
        }
    }
}
